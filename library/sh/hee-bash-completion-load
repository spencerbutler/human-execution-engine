#!/usr/bin/env bash
# urlid: hee-bash-completion-load:v2
# usage: source this from ~/.bashrc (bash only) to load HEE completions

if [[ -z "${BASH_VERSION-}" ]]; then
  return 0 2>/dev/null || true
fi

shopt -s progcomp 2>/dev/null || true

f="$HOME/.local/share/bash-completion/completions/hee"
[[ -r "$f" ]] && source "$f"
